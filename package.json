{
  "name": "eslint-config-precure",
  "version": "0.12.0",
  "description": "ESLint shareable config for precure",
  "license": "MIT",
  "repository": "akameco/eslint-config-precure",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "akameco.github.io"
  },
  "engines": {
    "node": ">=4"
  },
  "main": "index.js",
  "keywords": ["eslint", "prettier", "react", "flowtype", "jest"],
  "scripts": {
    "precommit": "lint-staged && npm run flow",
    "flow": "flow",
    "fmt": "prettier --write '*.{js,json,md}'",
    "lint": "eslint -c ./index.js index.js",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:jest": "eslint-find-rules --unused ./other/test/jest.js",
    "find-new-rules:flow": "eslint-find-rules --unused ./other/test/flow.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "eslint-check":
      "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "test": "npm run lint"
  },
  "lint-staged": {
    "*.js": ["fmt", "lint", "git add"],
    "*.{json,md}": ["fmt", "git add"]
  },
  "dependencies": {
    "all-contributors-cli": "^4.10.1",
    "babel-eslint": "^8.1.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-unicorn": "^3.0.1"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-find-rules": "^3.1.1",
    "flow-bin": "^0.60.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.2"
  },
  "peerDependencies": {
    "eslint": ">=3.0.0"
  }
}
