{
  "name": "eslint-config-precure",
  "version": "4.8.0",
  "description": "ESLint shareable config for precure",
  "license": "MIT",
  "repository": "akameco/eslint-config-precure",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "https://akameco.github.io"
  },
  "engines": {
    "node": ">=8"
  },
  "main": "index.js",
  "keywords": [
    "eslint",
    "prettier",
    "react",
    "flowtype",
    "jest",
    "typescript"
  ],
  "scripts": {
    "fmt": "prettier --write '*.{js,json,md}'",
    "lint": "eslint -c ./index.js index.js",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:jest": "eslint-find-rules --unused ./other/test/jest.js",
    "find-new-rules:jest:link": "node ./other/find-rules ./other/test/jest.js",
    "find-new-rules:flow": "eslint-find-rules --unused ./other/test/flow.js",
    "find-new-rules:flow:link": "node ./other/find-rules ./other/test/jest.js",
    "find-new-rules:ts": "eslint-find-rules --unused ./other/test/ts.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "find-new-rules:main:link": "node ./other/find-rules index.js",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "test": "yarn lint && yarn run find-new-rules"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.2",
    "eslint-plugin-unicorn": "^7.1.0",
    "ptils": "^0.3.0"
  },
  "devDependencies": {
    "@akameco/eslint-find-rules": "^3.4.0",
    "all-contributors-cli": "6.1.1",
    "eslint": "5.14.0",
    "eslint-rule-docs": "1.1.63",
    "flow-bin": "0.93.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.4",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "typescript": "3.3.3"
  },
  "peerDependencies": {
    "eslint": ">=3.0.0",
    "typescript": ">=3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run flow && lint-staged"
    }
  }
}
